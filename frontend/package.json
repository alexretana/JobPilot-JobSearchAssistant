{
  "name": "jobpilot-openmanus-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:autostart": "npx playwright test --config=playwright.config.ts",
    "docs": "cd .. && npx typedoc --out docs/frontend"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.54",
    "solid-js": "^1.9.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "express": "^5.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.5",
    "vite-plugin-solid": "^2.10.3",
    "vitest": "^3.2.4"
  }
}
